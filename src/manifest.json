{
  "manifest_version": 3,
  "name": "Download All Pdfs",
  "version": "1.0.0",
  "description": "Your ultimate solution for swiftly downloading PDFs",
  "background": {
    "service_worker": "background.js"
  },
  "action": {
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "16": "16.png",
    "48": "48.png",
    "128": "128.png"
  },
  "permissions": [
    "tabs",
    "downloads",
    "scripting",
    "storage"
  ],
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "options_page": "options.html"
}
