{
  "manifest_version": 3,
  "name": "Download All Pdfs",
  "version": "1.0.0",
  "description": "A simple Chrome extension built with Node.js and Webpack.",
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "16.png",
    "48": "48.png",
    "128": "128.png"
  },
  "permissions": [
    "storage",
    "tabs",
    "activeTab",
    "downloads"
  ],
  "options_page": "options.html",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}
